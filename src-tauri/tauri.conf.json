{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1423",
    "distDir": "../dist"
  },
  "package": {
    "productName": "riy-datos-t1",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "pe.riyapp.riy-datos-t1",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "windows": [
      {
        "title": "RIY Datos T1",
        "width": 800,
        "height": 600,
        "fullscreen": false,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    }
  }
}